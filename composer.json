{
  "name": "glook/isolated-composer",
  "description": "Tool for re-namespacing composer packages and source code to make it isolated",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "dev",
  "keywords": [
    "namespace",
    "wordpress",
    "composer"
  ],
  "authors": [
    {
      "name": "Andrey Polyakov",
      "email": "andrey@polyakov.im"
    },
    {
      "name": "Jon Gilkison",
      "email": "jon@interfacelab.com"
    }
  ],
  "require": {
    "symfony/console": "^5.1",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Glook\\IsolatedComposer\\": "src"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "bin": ["bin/namespacer"]
}
